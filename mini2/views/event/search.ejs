<div class="col-lg-12 col-sm-12">
	<div class="col-lg-6 col-sm-12">
	<div class="row">
		<% events.forEach( function(model) { %>
		<div class="col-lg-12 col-sm-12 panel panel-default">
			<div class="panel-heading">
				<strong><p class="panel-title"><a href="eventdetail/<%=model.id%>"><%=model.name%></a></p></strong>
			</div>
			<div class="panel-body">
				<div class="col-xs-4">
					<img src="<%=model.image%>" class="img-rounded img-respo:100px;object-fit:cover;" style="height: 150px;
					width: 100px;"></img>
				</div>
				<div class="col-xs-8" style="min-height:140px;max-height:140px;overflow:hidden" >
					<%=model.shortDes%>
				</div>
			</div>	
		</div>
		<% }); %>
	</div>
	</div>
	<nav>
		<ul class="pagination">
		<!-- <li>
				<a href="#" aria-label="Previous">
					<span aria-hidden="true">&laquo;</span>
				</a>
			</li> -->
			<% for (i = 1; i <= count; i++) { %>
			<li><a href="/Event/<%=page%>/?page=<%=i%>&<%=qs%>"> <%=i%> </a></li>
			<%}%>
		<!-- <li>
				<a href="#" aria-label="Next">
					<span aria-hidden="true">&raquo;</span>
				</a>
			</li> -->
		</ul>
	</nav>
	<strong><h2 class="control-label col-lg-6 col-sm-10 col-offset-1">Event Search form</h2></strong>
	<div class="col-lg-6 col-sm-12">
	<form action="/Event/search" method="GET" class="form-horizontal">
		<div class="form-group">
			<label class="control-label col-sm-2">Event Name: </label>
			<div class="col-sm-8">
				<input class="form-control" name="name" type="text" placeholder="Search name here">
			</div>
		</div>		
		<div class="form-group">
			<label class="control-label col-sm-2">venue:</label>
			<div class="col-sm-8">				
				<select class="form-control" id="venue" name="venue">
					<option value=""></option>
					<option>SWT501</option>
					<option>RRS638</option>
					<option>AC Hall</option>
				</select>							
			</div>
		</div>
		<div class="form-group">
			<label class="control-label col-sm-2">organizer:</label>
			<div class="col-sm-8">
				<select id="organizer" class="form-control" name="organizer">
					<option value="">please pick from below</option>
					<option value="gis">gis</option>
					<option value="Music Society">Music Society</option>
					<option value="comp">comp</option>
				</select>
			</div>
		</div>
		<div class="form-group">
			<label class="control-label col-sm-2">date:</label>
			<div class="col-sm-8">
				<input class="form-control" name="date" type="date">
			</div>
		</div>
		<button class="btn btn-default col-sm-offset-2" type="submit" value="ADD">Submit</button>
	</form>
	</div>
</div>
